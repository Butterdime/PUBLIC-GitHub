{
  "service_name": "human_ai_framework",
  "version": "1.0.0",
  "environment": "production",
  "features": {
    "authentication": true,
    "rate_limiting": true,
    "caching": false,
    "monitoring": true
  },
  "limits": {
    "max_requests_per_minute": 1000,
    "max_concurrent_connections": 100,
    "timeout_seconds": 30
  },
  "integrations": {
    "database": "postgresql",
    "cache": "redis",
    "queue": "rabbitmq"
  },
